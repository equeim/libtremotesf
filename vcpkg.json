{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "libtremotesf",
  "version": "0",
  "dependencies": [
    "fmt"
  ],
  "features": {
    "qt5": {
      "description": "Build with Qt 5",
      "supports": "!android",
      "dependencies": [
        {
          "name": "qt5-base",
          "default-features": false
        }
      ]
    },
    "qt6": {
      "description": "Build with Qt 6",
      "supports": "!android",
      "dependencies": [
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "concurrent",
            "network",
            "openssl",
            "testlib"
          ]
        }
      ]
    },
    "qt6-tremotesf-android": {
      "description": "Build with Qt 6 for tremotesf-android",
      "supports": "android",
      "dependencies": [
        "qtbase-tremotesf-android",
        {
          "name": "qtbase",
          "default-features": false,
          "host": true
        }
      ]
    }
  },
  "default-features": [],
  "overrides": [
    {
      "name": "qt5-base",
      "version": "5.15.8",
      "port-version": 3
    },
    {
      "name": "qtbase",
      "version": "6.4.2",
      "port-version": 0
    },
    {
      "name": "qtbase-tremotesf-android",
      "version": "6.4.2",
      "port-version": 0
    },
    {
      "name": "fmt",
      "version": "9.1.0",
      "port-version": 1
    }
  ]
}
