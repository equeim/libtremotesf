{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "libtremotesf",
  "version": "0",
  "supports": "windows,android",
  "dependencies": [
    "fmt",
    "cpp-httplib",
    {
      "name": "libpsl",
      "default-features": false,
      "features": ["libidn2"]
    },
    {
      "name": "pkgconf",
      "platform": "windows"
    }
  ],
  "features": {
    "qt5": {
      "description": "Build with Qt 5",
      "supports": "windows",
      "dependencies": [
        {
          "name": "qt5-base",
          "default-features": false
        }
      ]
    },
    "qt6": {
      "description": "Build with Qt 6",
      "dependencies": [
        {
          "name": "qtbase",
          "default-features": false,
          "host": true
        },
        {
          "name": "qtbase",
          "default-features": false,
          "features": [
            "concurrent",
            "network",
            "openssl",
            "testlib"
          ],
          "platform": "!android"
        },
        {
          "name": "qtbase-tremotesf-android",
          "platform": "android"
        }
      ]
    }
  },
  "default-features": [],
  "overrides": [
    {
      "name": "qtbase",
      "version": "6.4.3",
      "port-version": 0
    },
    {
      "name": "qtbase-tremotesf-android",
      "version": "6.4.3",
      "port-version": 0
    }
  ]
}
